{
  "name": "restaurant-server",
  "version": "1.0.0",
  "description": "Fastify starter project with TypeScript and ESM",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run postbuild",
    "postbuild": "node scripts/fix-imports.js",
    "start": "node --enable-source-maps dist/index.js",
    "dev": "tsx watch src/index.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "NODE_OPTIONS='--import tsx' node --no-warnings=ExperimentalWarning src/db/seed.ts"
  },
  "keywords": [
    "fastify",
    "typescript",
    "esm",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/formbody": "^8.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/websocket": "^11.1.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "express": "^4.21.2",
    "fastify": "^5.4.0",
    "fastify-plugin": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.7.1",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.2",
    "pino-pretty": "^13.0.0",
    "supertokens-node": "^23.0.0",
    "zod": "^3.25.67",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/luxon": "^3.7.1",
    "@types/node": "^20.19.1",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.4",
    "drizzle-kit": "^0.31.2",
    "rimraf": "^5.0.10",
    "tsx": "^3.14.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
